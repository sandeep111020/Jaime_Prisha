<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.jaimeprisha">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JaimePrisha">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SingleItem" />
        <activity android:name=".AddStatus" />
        <activity android:name=".NewOrdersScreen" />
        <activity android:name=".MyOrdersScreen" />
        <activity android:name=".StatusScreen" />
        <activity android:name=".PaymentScreen" />
        <activity android:name=".BookingScreen" />
        <activity android:name=".MyCartActivity" />
        <activity android:name=".FavoritesActivity" />
        <activity android:name=".DisplayItems" />
        <activity android:name=".ItemUploadScreen" />
        <activity android:name=".CollectionsScreen" />
        <activity android:name=".CategoriesScreen" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".MainActivity">

        </activity>

        <service
            android:name=".Notifications.FirebaseMessagingService"
            android:permission="TODO"
            tools:ignore="ExportedService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>